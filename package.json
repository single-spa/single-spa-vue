{
  "name": "single-spa-vue",
  "version": "1.10.1",
  "description": "a single-spa plugin for vue.js applications",
  "main": "lib/single-spa-vue.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "rimraf lib && rollup -c",
    "test": "concurrently -n w: 'yarn:test:*'",
    "test:integration": "cross-env BABEL_ENV=test jest",
    "test:types": "tsd",
    "prettier": "prettier --write './**'",
    "lint": "eslint src"
  },
  "moduleNameMapper": {
    "vue$": "vue/dist/vue"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently -n yarn:test yarn:lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/single-spa/single-spa-vue.git"
  },
  "files": [
    "lib",
    "src",
    "types"
  ],
  "types": "types/single-spa-vue.d.ts",
  "keywords": [
    "single-spa",
    "vue",
    "single",
    "page",
    "app",
    "spa"
  ],
  "author": "single-spa core team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/single-spa/single-spa-vue/issues"
  },
  "homepage": "https://github.com/single-spa/single-spa-vue#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.2",
    "@vue/test-utils": "^1.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css.escape": "^1.5.1",
    "eslint": "^6.8.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.12",
    "tsd": "^0.13.1"
  },
  "tsd": {
    "compilerOptions": {
      "lib": [
        "dom"
      ]
    }
  }
}
